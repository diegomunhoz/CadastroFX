<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.input.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="controller.MenuController">
  <children>
    <MenuBar layoutX="0.0" layoutY="2.0" prefWidth="600.0">
      <menus>
        <Menu mnemonicParsing="false" text="Cadastro">
          <items>
            <MenuItem mnemonicParsing="false" onAction="#cadastroCliente" text="Cliente">
              <accelerator>
                <KeyCodeCombination alt="UP" code="DIGIT1" control="UP" meta="UP" shift="DOWN" shortcut="UP" />
              </accelerator>
            </MenuItem>
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Consulta">
          <items>
            <MenuItem mnemonicParsing="false" onAction="#consultaCliente" text="Cliente">
              <accelerator>
                <KeyCodeCombination alt="UP" code="S" control="UP" meta="UP" shift="DOWN" shortcut="UP" />
              </accelerator>
            </MenuItem>
          </items>
        </Menu>
      </menus>
    </MenuBar>
  </children>
</AnchorPane>
